{
    "name": "delboy1978uk/bone-firewall",
    "description": "Firewall package for Bone Framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Derek Stephen McLean",
            "email": "delboy1978uk@gmail.com"
        }
    ],
    "require": {
        "php": ">7.2",
        "delboy1978uk/barnacle": "^2.3",
        "delboy1978uk/bone-http": "^2.0",
        "delboy1978uk/bone-router": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^2.0",
        "codeception/module-phpbrowser": "^1.0.0",
        "laminas/laminas-diactoros": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "Bone\\Firewall\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bone\\Test\\": "tests/unit/"
        }
    }
}
